page{
    background: #F2F2F2;
}
.my_login{
    background: #fff;
    height: 154rpx;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 30rpx;
}
.my_login_img image{
    width: 90rpx;
    height: 90rpx;
}
.my_login_img{
    float: left;
}
.my_login_text{
    float: left;
    font-size: 40rpx;
    line-height: 90rpx;
    color: #383838;
    margin-left: 30rpx;
}
.my_swiper{
    display: inline-block;
    width: 24%;
    text-align: center;
    height: 135rpx;
    margin-top: 40rpx;
}
.my_icon_img image{
    width: 90rpx;
    height: 90rpx;
}
.my_icon_name{
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}